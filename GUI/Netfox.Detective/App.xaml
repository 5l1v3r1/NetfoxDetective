<Application x:Class="Netfox.Detective.App" 
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:resources="clr-namespace:Netfox.Detective.Views.Resources">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Views/Styles/DetectiveMainStyle.xaml" />
                <ResourceDictionary Source="Views/Resources/Converters.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <resources:NullCommand x:Key="NullCommand" />
        </ResourceDictionary>
    </Application.Resources>
</Application>